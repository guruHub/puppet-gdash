Listen 80
<VirtualHost *:80>

    RackAutoDetect On
    ServerName <%= vhost %> 
    DocumentRoot <%= gdashroot %>

    ErrorLog 	/var/log/<%= service %>/errorlog-gdash
    CustomLog 	/var/log/<%= service %>/accesslog-gdash combined
  
    <Directory <%= gdashroot %> >
        Options None
        AllowOverride None
        Order allow,deny
        allow from all
    </Directory>

</VirtualHost>


